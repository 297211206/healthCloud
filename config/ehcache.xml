<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false" monitoring="autodetect" dynamicConfig="true">
    <diskStore path="java.io.tmpdir"/>
    <defaultCache maxElementsInMemory="10000" eternal="true" timeToIdleSeconds="18000000000" memoryStoreEvictionPolicy="LRU"/>
    <cache name="util.caches" maxElementsInMemory="1000" maxElementsOnDisk="100000" eternal="true" timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="true"/> 
</ehcache>
