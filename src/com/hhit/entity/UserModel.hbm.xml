<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hhit.entity">

	<class name="UserModel" table="t_user">
		<id name="uuid" type="long">
			<column name="c_uid"></column>
            <generator class="native"/>
		</id>		
		<property name="name" type="string" column="c_name"></property>			
		 <property name="accessToken" type="string" column="c_access_token"></property>
		<property name="openId" type="string" column="c_openid"></property>
		<property name="resource" type="Integer" column="c_resource"></property>  
		<property name="password" type="string" column="c_password"></property> 
		<property name="realName" type="string" column="c_realName"></property> 
		<property name="email" type="string" column="c_email"></property> 
		<property name="picName" type="string" column="c_picName"></property>
		<property name="lastLoginDate" type="string" column="c_last_login_date"></property>
		<property name="createDate" column="c_create_date">	</property>		   
	</class>
</hibernate-mapping>